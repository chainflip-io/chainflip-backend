{
  "name": "swap",
  "version": "0.0.21",
  "description": "Swap service backend",
  "private": true,
  "scripts": {
    "build": "pnpm clean && prisma generate && tsc && tsc-alias",
    "clean": "rm -rf dist",
    "dev": "tsx watch -r envlocker/config src/index.ts",
    "dev:berghain": "START_PROCESSOR=TRUE START_HTTP_SERVICE=TRUE ENVLOCKER_ENV_NAME=berghain pnpm dev",
    "dev:perseverance": "START_PROCESSOR=TRUE START_HTTP_SERVICE=TRUE ENVLOCKER_ENV_NAME=perseverance pnpm dev",
    "dev:sisyphos": "START_PROCESSOR=TRUE START_HTTP_SERVICE=TRUE ENVLOCKER_ENV_NAME=sisyphos pnpm dev",
    "dev:backspin": "START_PROCESSOR=TRUE START_HTTP_SERVICE=TRUE ENVLOCKER_ENV_NAME=backspin pnpm dev",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm eslint --max-warnings 0 './**/*.ts'",
    "postinstall": "prisma generate",
    "prettier:write": "pnpm prettier --write ./**/*",
    "start": "node dist/swap/src/index.js",
    "test:ci": "CI=1 jest --runInBand --forceExit",
    "test": "jest --runInBand",
    "typecheck": "tsc --noEmit",
    "generate:models": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate",
    "graphql:codegen": "graphql-codegen --config .graphqlrc.js"
  },
  "keywords": [
    "typescript"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/graphql-fields": "^1.3.9",
    "@types/qs": "^6.9.11",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "eslint-plugin-n": "^16.6.2",
    "graphql": "^16.8.1",
    "prisma": "^5.9.1",
    "socket.io-client": "^4.7.4",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": "20.11.0",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@polkadot/util": "^12.6.2",
    "@polkadot/util-crypto": "^12.6.2",
    "@prisma/client": "^5.9.1",
    "axios": "^1.6.7",
    "bignumber.js": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ethers": "^6.9.2",
    "express": "^4.18.2",
    "graphql-request": "^6.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "qs": "^6.11.2",
    "rxjs": "^7.8.1",
    "safe-stable-stringify": "^2.4.3",
    "socket.io": "^4.7.2",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  }
}
