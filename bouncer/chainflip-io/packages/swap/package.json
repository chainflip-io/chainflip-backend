{
  "name": "swap",
  "version": "0.0.8",
  "description": "Swap service backend",
  "private": true,
  "scripts": {
    "build": "pnpm clean && prisma generate && tsc && tsc-alias",
    "clean": "rm -rf dist",
    "dev": "tsx watch -r envlocker/config src/index.ts",
    "dev:sisyphos": "ENVLOCKER_ENV_NAME=sisyphos pnpm dev",
    "dev:partnernet": "ENVLOCKER_ENV_NAME=partnernet pnpm dev",
    "dev:backspin": "ENVLOCKER_ENV_NAME=backspin pnpm dev",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm eslint --max-warnings 0 './**/*.ts'",
    "postinstall": "prisma generate",
    "prettier:write": "pnpm prettier --write ./**/*",
    "start": "node dist/swap/src/index.js",
    "test:ci": "CI=1 jest --runInBand --forceExit",
    "test": "jest --runInBand",
    "typecheck": "tsc --noEmit",
    "generate:models": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate",
    "graphql:codegen": "graphql-codegen --config .graphqlrc.js"
  },
  "keywords": [
    "typescript"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/graphql-fields": "^1.3.4",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.4",
    "graphql": "^16.6.0",
    "prisma": "^4.16.1",
    "socket.io-client": "^4.6.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": "18.16.0",
    "pnpm": ">=7"
  },
  "dependencies": {
    "@polkadot/util": "^12.0.1",
    "@polkadot/util-crypto": "^12.0.1",
    "@prisma/client": "^4.16.1",
    "bignumber.js": "^9.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "express": "^4.18.2",
    "graphql-request": "^6.0.0",
    "pg": "^8.10.0",
    "qs": "^6.11.1",
    "rxjs": "^7.8.0",
    "socket.io": "^4.6.1",
    "winston": "^3.8.2",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  }
}
